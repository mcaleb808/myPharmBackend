{
  "name": "mypharm",
  "version": "1.0.0",
  "description": "List local pharmacies",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./api/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "test": "cross-env NODE_ENV=test jest   --coverage --testTimeout=10000 --detectOpenHandles --forceExit",
    "prettier": "eslint --ignore-path ./.eslintignore . --fix",
    "build": "rm -rf ./build && babel -d ./build ./api -s",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "author": "Caleb Mugisha",
  "contributors": ["Musigwa Pacifique"],
  "license": "ISC",
  "dependencies": {
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "pg": "7.12.1",
    "pg-hstore": "2.3.3",
    "sequelize": "5.21.1"
  },
  "devDependencies": {
    "supertest": "4.0.2",
    "jest": "24.9.0",
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/node": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/register": "7.6.2",
    "@babel/runtime": "7.6.2",
    "babel-loader": "8.0.6",
    "coveralls": "3.0.7",
    "cross-env": "6.0.3",
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2"
  }
}
